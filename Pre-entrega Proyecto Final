    "                xytext=(5, 0),  # Offset the text slightly to the right\n",
    "                textcoords=\"offset points\",\n",
    "                ha='left', va='center')\n",
    "\n",
    "plt.gca().invert_yaxis()  # Invert the y-axis to show the highest percentage at the top\n",
    "plt.show()\n"
